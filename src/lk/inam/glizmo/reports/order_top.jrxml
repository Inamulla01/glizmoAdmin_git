	<queryString>
		<![CDATA[SELECT 
    `product_id`, 
    `product_name`, 
    SUM(`order_quantity`) AS `total_quantity_sold`,
    SUM(`order_price` * `order_quantity`) AS `total_revenue`
FROM 
    `glizmo_dbms`.`view_order`
GROUP BY 
    `product_id`, 
    `product_name`
ORDER BY 
    `total_quantity_sold` DESC
LIMIT 10
]]>
	</queryString>
